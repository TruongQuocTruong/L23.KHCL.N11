      "source": [
        "str = input().split()\n",
        "n = int(str[0])\n",
        "k = int(str[1])\n",
        "\n",
        "def Fibo(k):\n",
        "  x1 = 1\n",
        "  x2 = 1\n",
        "  i = 2\n",
        "  while i <= k:\n",
        "    Temp = x2\n",
        "    x2 = x1 + x2\n",
        "    x1 = Temp\n",
        "    i += 1\n",
        "  return x2\n",
        "print((n*Fibo(k*2))% (10**9 + 7))"
      ],
